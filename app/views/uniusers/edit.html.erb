<%= form_for @user, :url => uniuser_path(@user), :method => :patch do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %>
    <%= f.label :password, "Password" %>
    <%= f.text_area :password %>
   	<%= f.label :money, "Money" %>
    <%= f.text_field :money %>



    <%= f.submit "Update" %>
<% end %>